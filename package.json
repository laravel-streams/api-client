{
    "name": "@laravel-streams/api-client",
    "version": "1.1.0",
    "publishConfig": {
        "access": "public"
    },
    "types": "types/index.d.ts",
    "main": "dist/streams-api.cjs.js",
    "module": "dist/streams-api.esm-bundler.js",
    "scripts": {
        "build": "npm-run-all clean test bundle dts cli:analyze doc:api cli:docs",
        "clean": "npm-run-all clean:dist clean:doc:api",
        "clean:dist": "rm -rf ./dist ./types",
        "clean:doc:api": "rm -rf ./docs/api",
        "clean:src": "rimraf src/**/*.{js,js.map}",
        "cli:analyze": "ts-node ./scripts/cli.ts analyze",
        "cli:docs": "ts-node ./scripts/cli.ts docs",
        "doc:api": "typedoc --options ./typedoc.js",
        "bundle": "rollup --config rollup.config.ts --configPlugin typescript",
        "watch": "rollup --config rollup.config.ts --configPlugin typescript --watch",
        "dts": "tsc -p ./tsconfig.build.json --emitDeclarationOnly  --declaration --declarationDir types",
        "prepublish": "npm-run-all build",
        "test": "mocha"
    },
    "dependencies": {
        "camelcase": "^6.2.0",
        "collect.js": "^4.29.3",
        "deepmerge": "^4.2.2",
        "tapable": "^2.2.1"
    },
    "peerDependencies": {
        "axios": "^0.24.0",
        "qs": "^6.10.2"
    },
    "devDependencies": {
        "axios": "^0.24.0",
        "yargs": "^17.3.1",
        "@types/yargs": "^17.0.8",
        "markdown-it": "^12.3.0",
        "markdown-it-anchor": "^8.4.1",
        "@sindresorhus/slugify": "^2.1.0",
        "highlight.js": "^11.3.1",
        "@types/highlight.js": "^10.1.0",
        "@types/markdown-it": "^12.2.3",
        "@gerhobbelt/markdown-it-attrs": "^4.1.0-24",
        "qs": "^6.10.2",
        "@radic/console-input": "^1.10.0",
        "@radic/core": "^1.0.5",
        "@types/dot-object": "^2.1.2",
        "@types/fs-extra": "^9.0.13",
        "@types/jquery": "^3.5.10",
        "@types/lodash": "^4.14.177",
        "dot-object": "^2.1.4",
        "fs-extra": "^10.0.0",
        "jquery": "^3.6.0",
        "lodash": "^4.17.21",
        "reflect-metadata": "^0.1.13"
    },
    "bugs": {
        "url": "https://github.com/laravel-streams/api-client/issues"
    },
    "repository": {
        "url": "git@github.com:laravel-streams/api-client.git",
        "type": "git"
    },
    "homepage": "https://streams.dev",
    "description": "An API client for the Laravel Streams API package.",
    "keywords": [
        "laravel",
        "php",
        "streams",
        "laravel-streams",
        "crud",
        "api",
        "awesome"
    ],
    "license": "MIT",
    "author": {
        "url": "https://github.com/robinradic",
        "email": "rradic@hotmail.com",
        "name": "Robin Radic"
    },
    "contributors": [
        {
            "name": "Ryan Thompson",
            "email": "ryan@pyrocms.com",
            "url": "https://github.com/ryanthompson"
        }
    ]
}
