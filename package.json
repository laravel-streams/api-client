{
    "name": "@laravel-streams/api-client",
    "version": "2.0.4",
    "publishConfig": {
        "access": "public"
    },
    "source": "src/index.js",
    "main": "dist/index.js",
    "module": "dist/module.js",
    "legacy": "dist/legacy.js",
    "scripts": {
        "dev": "parcel src/index.html",
        "watch": "parcel watch",
        "build": "parcel build",
        "clean": "rm -rf dist",
        "test": "mocha"
    },
    "peerDependencies": {},
    "devDependencies": {
        "npx": "^10.2.2",
        "parcel": "^2.8.0"
    },
    "targets": {
        "default": {
            "isLibrary": true,
            "outputFormat": "esmodule",
            "engines": {
                "browsers": "Chrome 80"
            }
        },
        "legacy": {
            "isLibrary": true,
            "outputFormat": "commonjs",
            "engines": {
                "browsers": "> 0.5%, last 2 versions, not dead"
            }
        }
    },
    "bugs": {
        "url": "https://github.com/laravel-streams/api-client/issues"
    },
    "repository": {
        "url": "git@github.com:laravel-streams/api-client.git",
        "type": "git"
    },
    "homepage": "https://streams.dev",
    "description": "An API client for the Laravel Streams API package.",
    "keywords": [
        "laravel",
        "php",
        "streams",
        "laravel-streams",
        "crud",
        "api",
        "awesome"
    ],
    "license": "MIT",
    "author": {
        "url": "https://github.com/robinradic",
        "email": "rradic@hotmail.com",
        "name": "Robin Radic"
    },
    "contributors": [
        {
            "name": "Ryan Thompson",
            "email": "ryan@pyrocms.com",
            "url": "https://github.com/ryanthompson"
        }
    ]
}
