{
    "name": "@laravel-streams/api-client",
    "version": "3.0.0",
    "description": "A JavaScript client for the Laravel Streams API",
    "publishConfig": {
        "access": "public"
    },
    "source": "src/index.js",
    "main": "dist/index.js",
    "module": "dist/module.js",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/module.js"
        }
    },
    "scripts": {
        "dev": "python3 -m http.server 8080",
        "dev:parcel": "parcel playground.html --open",
        "watch": "parcel watch",
        "build": "parcel build",
        "clean": "rm -rf dist .parcel-cache",
        "test": "node --test test/*.test.js",
        "test:watch": "node --test --watch test/*.test.js"
    },
    "dependencies": {},
    "devDependencies": {
        "parcel": "^2.12.0"
    },
    "targets": {
        "main": {
            "source": "src/index.js",
            "outputFormat": "commonjs"
        },
        "module": {
            "source": "src/index.js",
            "outputFormat": "esmodule"
        }
    },
    "bugs": {
        "url": "https://github.com/laravel-streams/api-client/issues"
    },
    "repository": {
        "url": "git@github.com:laravel-streams/api-client.git",
        "type": "git"
    },
    "homepage": "https://streams.dev",
    "keywords": [
        "laravel",
        "streams",
        "api-client",
        "crud"
    ],
    "license": "MIT",
    "author": {
        "name": "Robin Radic",
        "email": "rradic@hotmail.com",
        "url": "https://github.com/robinradic"
    },
    "contributors": [
        {
            "name": "Ryan Thompson",
            "email": "ryan@pyrocms.com",
            "url": "https://github.com/ryanthompson"
        }
    ]
}
